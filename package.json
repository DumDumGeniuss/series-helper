{
  "name": "react-universal-redux-starter-kit",
  "version": "1.0.0",
  "description": "It's the react-start-kit for you to quickly build application of React Isomorphic Structure.",
  "scripts": {
    "eslint": "better-npm-run eslint",
    "start:dev": "better-npm-run start:dev",
    "build": "better-npm-run build",
    "start:prod": "better-npm-run start:prod",
    "start:build": "better-npm-run start:build"
  },
  "betterScripts": {
    "eslint": {
      "command": "eslint src; exit 0"
    },
    "start:dev": {
      "command": "webpack --config tools/webpack.dev.config.js && babel-node src/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:build": {
      "command": "webpack --config tools/webpack.prod.config.js && babel-node src/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config tools/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:prod": {
      "command": "babel-node src/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "DumDumGeniuss",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-icons": "^2.2.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.26.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
